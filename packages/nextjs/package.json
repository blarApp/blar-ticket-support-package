{
  "name": "@blario/nextjs",
  "version": "0.2.1-alpha.0",
  "description": "Drop-in issue reporter for Next.js applications with AI-powered diagnostics",
  "author": "Blario",
  "license": "MIT",
  "homepage": "https://github.com/blarApp/blar-ticket-support-package",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blarApp/blar-ticket-support-package.git",
    "directory": "packages/nextjs"
  },
  "bugs": {
    "url": "https://github.com/blarApp/blar-ticket-support-package/issues"
  },
  "keywords": [
    "blario",
    "nextjs",
    "react",
    "issue-reporter",
    "diagnostics",
    "bug-reporting",
    "support"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./theme.css": "./dist/styles/theme.css",
    "./tailwind-preset": "./tailwind-preset.js",
    "./styles.css": "./dist/blario.css"
  },
  "files": [
    "dist",
    "tailwind-preset.js",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build:css": "postcss src/styles/lib.css -o dist/blario.css",
    "build": "tsup && npm run build:css",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "size": "size-limit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "cmdk": ">=1.0.0",
    "lucide-react": ">=0.300.0",
    "next": ">=13.4.0",
    "next-themes": ">=0.2.0",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "react-hook-form": ">=7.0.0",
    "sonner": ">=1.0.0",
    "tailwindcss": ">=3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": ">=0.9.4",
    "zod": "^3.22.4 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "tailwindcss-animate": {
      "optional": true
    },
    "cmdk": {
      "optional": true
    },
    "vaul": {
      "optional": true
    },
    "react-hook-form": {
      "optional": true
    },
    "zod": {
      "optional": true
    },
    "sonner": {
      "optional": true
    },
    "lucide-react": {
      "optional": true
    },
    "next-themes": {
      "optional": true
    }
  },
  "devDependencies": {
    "@blario/core": "workspace:*",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.1.3",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-dropdown-menu": "2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "2.1.1",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-separator": "1.1.1",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "1.1.2",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "@size-limit/preset-small-lib": "^11.0.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "embla-carousel-react": "^8.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^12.10.3",
    "input-otp": "^1.2.4",
    "lucide-react": "^0.309.0",
    "msw": "^2.0.11",
    "next": "^14.0.4",
    "next-themes": "^0.3.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-prefix-selector": "^2.1.1",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-day-picker": "^9.2.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.5",
    "react-resizable-panels": "^2.0.21",
    "recharts": "^2.10.3",
    "size-limit": "^11.0.1",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^4.1.13",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.0.1",
    "tw-animate-css": "1.3.3",
    "typescript": "^5.3.3",
    "vaul": "^0.9.4",
    "vitest": "^1.1.1",
    "zod": "^3.22.4"
  },
  "size-limit": [
    {
      "path": "dist/index.mjs",
      "limit": "70 kB"
    }
  ],
  "publishConfig": {
    "access": "public"
  }
}
